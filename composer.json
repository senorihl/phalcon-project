{
    "name": "senorihl/phalcon-project",
    "description": "A sample development phalcon project",
    "type": "project",
    "license": "MIT",
    "config": {
        "platform": {
            "php": "8.4"
        }
    },
    "require": {
        "ext-phalcon": "^5.4",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-mbstring": "*",
        "ext-yaml": "*",
        "symfony/console": "^7.2",
        "nette/php-generator": "^4.1",
        "doctrine/sql-formatter": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "files": [ "src/services.php" ]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phalcon/ide-stubs": "^5.8",
        "symfony/var-dumper": "^6.4",
        "phpunit/phpunit": "^12.0"
    }
}
